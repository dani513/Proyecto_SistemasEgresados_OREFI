{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'SearchEdit',\n  data() {\n    return {\n      cedula: '',\n      egresado: null\n    };\n  },\n  methods: {\n    async searchGraduate() {\n      const response = await fetch(`/api/consultar?cedula=${this.cedula}`);\n      if (response.ok) {\n        const egresados = await response.json();\n        if (egresados.length > 0) {\n          this.egresado = egresados[0];\n        } else {\n          alert('No se encontró un egresado con esa cédula');\n        }\n      } else {\n        alert('Error al buscar egresado');\n      }\n    },\n    async editGraduate() {\n      const response = await fetch(`/api/editar/${this.egresado.id}`, {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify(this.egresado)\n      });\n      if (response.ok) {\n        alert('Egresado editado con éxito');\n        this.$router.push('/main');\n      } else {\n        alert('Error al editar egresado');\n      }\n    },\n    cancel() {\n      this.$router.push('/main');\n    }\n  }\n};","map":{"version":3,"names":["name","data","cedula","egresado","methods","searchGraduate","response","fetch","ok","egresados","json","length","alert","editGraduate","id","method","headers","body","JSON","stringify","$router","push","cancel"],"sources":["src/components/SearchEdit.vue"],"sourcesContent":["<template>\n    <div class=\"search-edit\">\n      <h1>Buscar y Editar Egresado</h1>\n      <div>\n        <label for=\"cedula\">Cédula</label>\n        <input type=\"text\" v-model=\"cedula\" @keyup.enter=\"searchGraduate\">\n        <button @click=\"searchGraduate\">Buscar</button>\n      </div>\n      <div v-if=\"egresado\">\n        <h2>Editar Egresado</h2>\n        <form @submit.prevent=\"editGraduate\">\n          <div>\n            <label for=\"nombre\">Nombre</label>\n            <input type=\"text\" v-model=\"egresado.nombre\">\n          </div>\n          <div>\n            <label for=\"cod_carrera\">Código de Carrera</label>\n            <input type=\"text\" v-model=\"egresado.cod_carrera\">\n          </div>\n          <div>\n            <label for=\"cod_periodo\">Código de Período</label>\n            <input type=\"text\" v-model=\"egresado.cod_periodo\">\n          </div>\n          <div>\n            <label for=\"cedula\">Cédula</label>\n            <input type=\"text\" v-model=\"egresado.cedula\">\n          </div>\n          <!-- Añade más campos según sea necesario -->\n          <div class=\"buttons\">\n            <button type=\"button\" @click=\"cancel\">Cancelar</button>\n            <button type=\"submit\">Actualizar</button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    name: 'SearchEdit',\n    data() {\n      return {\n        cedula: '',\n        egresado: null\n      }\n    },\n    methods: {\n      async searchGraduate() {\n        const response = await fetch(`/api/consultar?cedula=${this.cedula}`);\n        if (response.ok) {\n          const egresados = await response.json();\n          if (egresados.length > 0) {\n            this.egresado = egresados[0];\n          } else {\n            alert('No se encontró un egresado con esa cédula');\n          }\n        } else {\n          alert('Error al buscar egresado');\n        }\n      },\n      async editGraduate() {\n        const response = await fetch(`/api/editar/${this.egresado.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(this.egresado)\n        });\n        if (response.ok) {\n          alert('Egresado editado con éxito');\n          this.$router.push('/main');\n        } else {\n          alert('Error al editar egresado');\n        }\n      },\n      cancel() {\n        this.$router.push('/main');\n      }\n    }\n  }\n  </script>\n  \n  <style scoped>\n  .search-edit {\n    text-align: center;\n    margin-top: 50px;\n  }\n  button {\n    padding: 10px 20px;\n    font-size: 16px;\n    cursor: pointer;\n    margin: 5px;\n  }\n  input {\n    margin-top: 10px;\n    padding: 10px;\n    font-size: 16px;\n  }\n  form div {\n    margin-bottom: 10px;\n  }\n  .buttons {\n    display: flex;\n    justify-content: center;\n  }\n  .buttons button {\n    margin: 0 10px;\n  }\n  </style>\n  "],"mappings":";AAsCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,eAAA;MACA,MAAAC,QAAA,SAAAC,KAAA,+BAAAL,MAAA;MACA,IAAAI,QAAA,CAAAE,EAAA;QACA,MAAAC,SAAA,SAAAH,QAAA,CAAAI,IAAA;QACA,IAAAD,SAAA,CAAAE,MAAA;UACA,KAAAR,QAAA,GAAAM,SAAA;QACA;UACAG,KAAA;QACA;MACA;QACAA,KAAA;MACA;IACA;IACA,MAAAC,aAAA;MACA,MAAAP,QAAA,SAAAC,KAAA,qBAAAJ,QAAA,CAAAW,EAAA;QACAC,MAAA;QACAC,OAAA;UACA;QACA;QACAC,IAAA,EAAAC,IAAA,CAAAC,SAAA,MAAAhB,QAAA;MACA;MACA,IAAAG,QAAA,CAAAE,EAAA;QACAI,KAAA;QACA,KAAAQ,OAAA,CAAAC,IAAA;MACA;QACAT,KAAA;MACA;IACA;IACAU,OAAA;MACA,KAAAF,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}